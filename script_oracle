#!/bin/bash

/OPT/orabase/product/19.0.0.0/bin/sqlplus / as sysdba<<!
SPOOL ${0%sh}log
SELECT LOCALTIMESTAMP FROM DUAL;

ALTER SESSION SET CURRENT_SCHEMA = SCHEMA_NAME;

UPDATE TABLE SET COLONNE_1=1 WHERE DATE_COLL_2 < TO_DATE('2023-02-06 00:00:00', 'YYYY-MM-DD HH24:MI:SS');
COMMIT;

SELECT LOCALTIMESTAMP FROM DUAL;
SPOOL OFF
EXIT
!
